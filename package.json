{
  "type": "module",
  "name": "pinstripe",
  "description": "Pinstripe is a fullstack JavaScript web framework for Node.js and the browser that follows the model-view-controller (MVC) design pattern.",
  "version": "0.0.0",
  "exports": {
    ".": "./lib/index.js"
  },
  "bin": {
    "pinstripe": "./cli.js"
  },
  "license": "MIT",
  "scripts": {
    "build": "rollup --config",
    "watch": "nodemon -e js,scss --exec 'yarn build && yarn start' --ignore pinstripe.js",
    "clean": "rm -rf ./node_modules && rm -rf ./lib/static/javascripts/pinstripe.js*",
    "test": "yarn test:unit",
    "test:unit": "jest lib"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-jest": "^25.1.0",
    "jest": "^26.6",
    "ramda": "^0.27.0",
    "rollup": "^2.0.2"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "co-body": "^6.1.0",
    "commonmark": "^0.29.3",
    "mime-types": "^2.1.28",
    "mysql": "^2.18.1",
    "qs": "^6.9.4"
  }
}
