    "co-body": "^6.1.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^12.0.6",
    "memfs": "^3.2.2",
    "mime-types": "^2.1.28",
    "mysql": "^2.18.1",
    "qs": "^6.9.4",
    "unionfs": "^4.4.0",
    "webpack": "^5.39.1"
  }
